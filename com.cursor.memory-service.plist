<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.cursor.memory-service</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>-c</string>
        <string>cd "$HOME/Library/Mobile Documents/com~apple~CloudDocs/Documents/socialmediamktagent-v1-1" && ./start-memory-service.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <false/>
    <key>StandardOutPath</key>
    <string>/tmp/cursor-memory-service.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/cursor-memory-service-error.log</string>
    <key>StartInterval</key>
    <integer>7200</integer>
</dict>
</plist> 